<% const curr=typeof currentPage!='undefined'?currentPage:1; const total=typeof totalPages!='undefined'?totalPages:1; const q=typeof queryParams!='undefined'?queryParams:''; if(total>1){ %><nav class="mt-4"><ul class="pagination"><li class="<%= curr==1?'disabled':'' %>"><a href="?page=<%= curr-1 %><%= q %>">&laquo;</a></li><% for(let i=1;i<=total;i++){ %><li class="<%= curr==i?'active':'' %>"><a href="?page=<%= i %><%= q %>"><%= i %></a></li><% } %><li class="<%= curr==total?'disabled':'' %>"><a href="?page=<%= curr+1 %><%= q %>">&raquo;</a></li></ul></nav><% } %>